{
  "expo": {
    "name": "ScrollSong",
    "slug": "ScrollSong",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splash-screen.png",
    "userInterfaceStyle": "light",
    "scheme": "scrollsong",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vlanp.ScrollSong"
    },
    "android": {
      "package": "com.vlanp.ScrollSong"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Poppins-Bold.ttf",
            "./assets/fonts/Lato-Bold.ttf",
            "./assets/fonts/Lato-Heavy.ttf",
            "./assets/fonts/Lato-Semibold.ttf",
            "./assets/fonts/Lexend-Bold.ttf",
            "./assets/fonts/Lexend-Light.ttf",
            "./assets/fonts/Lexend-SemiBold.ttf",
            "./assets/fonts/Poppins-BoldItalic.ttf",
            "./assets/fonts/Poppins-Light.ttf",
            "./assets/fonts/Poppins-LightItalic.ttf",
            "./assets/fonts/Poppins-Medium.ttf",
            "./assets/fonts/Poppins-MediumItalic.ttf",
            "./assets/fonts/Poppins-Regular.ttf",
            "./assets/fonts/Poppins-SemiBold.ttf",
            "./assets/fonts/Poppins-SemiBoldItalic.ttf",
            "./assets/fonts/RobotoCondensed-Bold.ttf",
            "./assets/fonts/RobotoCondensed-Regular.ttf"
          ]
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash-screen.png",
          "enableFullScreenImage_legacy": true,
          "resizeMode": "cover"
        }
      ]
    ]
  }
}
